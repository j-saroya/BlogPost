<%- include("partials/header"); -%>

<h1>Compose</h1>

<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <input type="text" class="form-control" name="postTitle" value="">
    <label for="postBody">Post</label>
    <textarea name="postBody" rows="5" cols="30" class="form-control"></textarea>

  </div>
  <button type="submit" class="btn btn-primary" name="submit">Publish</button>
</form>

<%- include("partials/footer"); -%>
